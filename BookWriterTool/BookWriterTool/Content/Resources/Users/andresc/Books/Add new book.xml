<?xml version="1.0" encoding="utf-8"?>
<!-- start of book -->
<book>
  <!-- metadata, to group all the information about this book into one element. -->
  <!-- define our chapters -->
  <chapters>
    <!-- chapter1 - the first chapter to be seen -->
    <chapter id="chapter1">
      <!-- grouped chapter information -->
      <!-- a chapter can have one ore more pages. -->
      <pages>
        <!-- our first page! -->
        <page id="page1">
          <frames>
            <frame bordertype="rectangle" id="frame1">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame2">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame3">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
          </frames>
        </page>
        <page id="page2">
          <frames>
            <frame bordertype="rectangle" id="frame1">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame2">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame3">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
          </frames>
        </page>
      </pages>
      <!-- End of chapter -->
    </chapter>
    <!-- End of chapters -->
  </chapters>
  <!-- End of book -->
</book>