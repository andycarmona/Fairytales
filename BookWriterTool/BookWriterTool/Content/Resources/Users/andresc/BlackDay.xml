<?xml version="1.0" encoding="utf-8"?>
<!-- start of book -->
<book>
  <!-- metadata, to group all the information about this book into one element. -->
  <!-- define our chapters -->
  <chapters>
    <!-- chapter1 - the first chapter to be seen -->
    <chapter id="chapter1">
      <!-- grouped chapter information -->
      <!-- a chapter can have one ore more pages. -->
      <pages>
        <!-- our first page! -->
        <page id="page1">
          <frames>
            <frame bordertype="rectangle" id="frame1">
              <contents>
                <content target="rectangle" type="" background="background/purple-boardwalk-pier-jetty-pink-sunset-1920x1080-1024x576.jpg">
                  <objects>
                    <object img="female/girlInGreen.png" scaleX="25%" scaleY="25%" origo="25%" type="character2D" id="Cloned32" />
                    <object img="male/redFighter.png" scaleX="25%" scaleY="25%" origo="25%" type="character2D" id="Cloned78" />
                    <object img="male/redFighter.png" scaleX="25%" scaleY="25%" origo="25%" type="character2D" id="Cloned8" />
                  </objects>
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame2">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame3">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
          </frames>
        </page>
        <page id="page2">
          <frames>
            <frame bordertype="rectangle" id="frame1">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="rectangle" id="frame2">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="square" id="frame3">
              <contents>
                <content target="left" type="none" background="background/anime_joker-1024x576.jpg">
                  <objects>
                    <object img="female/girlInGreen.png" scaleX="25%" scaleY="25%" origo="25%" type="character2D" id="Cloned96" />
                  </objects>
                </content>
                <content target="right" type="none">
                  <objects />
                </content>
              </contents>
            </frame>
          </frames>
        </page>
        <page id="page3">
          <frames>
            <frame bordertype="rectangle" id="frame1">
              <contents>
                <content target="rectangle" type="">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame bordertype="square" id="frame2">
              <contents>
                <content target="left" type="none">
                  <objects />
                </content>
                <content target="right" type="none">
                  <objects />
                </content>
              </contents>
            </frame>
            <frame id="frame3" bordertype="square">
              <contents>
                <content target="left" background="">
                  <objects />
                </content>
                <content target="right" background="">
                  <objects />
                </content>
              </contents>
            </frame>
          </frames>
        </page>
      </pages>
      <!-- End of chapter -->
    </chapter>
    <!-- End of chapters -->
  </chapters>
  <!-- End of book -->
</book>