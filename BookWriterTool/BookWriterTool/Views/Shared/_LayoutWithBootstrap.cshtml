@using BookWriterTool.Helpers
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - BookEditor</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <!-- Le styles -->
           @Styles.Render("~/Content/css")
         @Styles.Render("~/Content/customcss")
            <link href="/Content/assets/css/bootstrap.min.css" rel="stylesheet">
            <link type="text/css" href="/Content/themes/custom-theme/jquery-ui-1.10.0.custom.css" rel="stylesheet" />
            <link type="text/css" href="/Content/assets/css/font-awesome.min.css" rel="stylesheet" />
            <!--[if IE 7]>
            <link rel="stylesheet" href="assets/css/font-awesome-ie7.min.css">
            <![endif]-->
            <!--[if lt IE 9]>
            <link rel="stylesheet" type="text/css" href="css/custom-theme/jquery.ui.1.10.0.ie.css"/>
            <![endif]-->
            <link href="/Content/assets/css/docs.css" rel="stylesheet">
            <link href="/Content/assets/js/google-code-prettify/prettify.css" rel="stylesheet">

            <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
            <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
            <![endif]-->
        <script src="/Content/assets/js/jquery-1.9.0.min.js" type="text/javascript"></script>
<script src="/Content/assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="/Content/assets/js/jquery-ui-1.10.0.custom.min.js" type="text/javascript"></script>
<script src="/Content/assets/js/google-code-prettify/prettify.js" type="text/javascript"></script>
<script src="/Content/assets/js/docs.js" type="text/javascript"></script>
<script type="text/javascript">
		var unityObjectUrl = "http://webplayer.unity3d.com/download_webplayer-3.x/3.0/uo/UnityObject2.js";
		if (document.location.protocol == 'https:')
			unityObjectUrl = unityObjectUrl.replace("http://", "https://ssl-");
		document.write('<script type="text\/javascript" src="' + unityObjectUrl + '"><\/script>');
		</script>
    </head>
    <body id="body">
         @RenderBody()

       
        @RenderSection("scripts", required: false)
    </body>
</html>
